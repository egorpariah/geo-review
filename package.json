{
  "name": "geo-review",
  "private": "true",
  "version": "1.0.0",
  "description": "Отзывы на карте с помощью Yandex Maps API и сервера на Express.js",
  "scripts": {
    "build": "npm run build -w @geo-review/frontend",
    "watch": "npm run watch -w @geo-review/frontend",
    "serve": "npm run serve -w @geo-review/backend",
    "start": "npm-run-all --parallel serve watch"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egorpariah/geo-review.git"
  },
  "author": "Egor Golikov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/egorpariah/geo-review/issues"
  },
  "homepage": "https://github.com/egorpariah/geo-review#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
